function [u,U,fhat] = GEBasic(A,f)

B = [A,f];
numRows = length(B(:,1));

for i = 1:numRows-1
    while(B(i,i)==0)
        temp = B(i,:);
        new = B(i+1,:);
        B(i,:) = new;
        B(i+1,:)= temp;
    end
    for j = i+1:numRows
        scaling = -B(j,i)/B(i,i);
        B(j,:)=B(i,:)*scaling+B(j,:);
    end
end

U = B(1:3,1:3);
fhat = B(:,4);

u = ones(numRows,1);
for i = 1:numRows
    sum = U*u;
    for j = 2:numRows
        
    end
    u(numRows+1-i)=sum(numRows+1-i);
end

